@model List<UserIntrest>

<hr />

<h1>Intresserade</h1>

@foreach (UserIntrest userinterest in Model)
{
    <div class="col col-md-12">
        <blockquote>
            <h5>@userinterest.UserName</h5>
        </blockquote>
    </div>
}

@if (Model.Count == 0)
{
    <div class="col col-md-12">
        <hr />
        <p>Inga har skrivit up sit på detta Event...</p>
    </div>
}
