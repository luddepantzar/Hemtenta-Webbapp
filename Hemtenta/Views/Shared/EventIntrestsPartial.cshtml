@model List<UserIntrest>


@foreach (UserIntrest userinterest in Model)
{
    <div class="col col-md-12">
        <blockquote>
            <h5>@userinterest.UserName</h5>
            <p><a asp-action="Delete" asp-controller="Reviews" asp-route-id="@userinterest.Id" class="btn btn-danger"> Ta bort</a></p>
        </blockquote>
    </div>
}

@if (Model.Count == 0)
{
    <div class="col col-md-12">
        <hr />
        <p>Inga har skrivit up sit på detta event...</p>
    </div>
}
